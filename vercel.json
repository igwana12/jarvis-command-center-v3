{
  "version": 2,
  "builds": [
    {
      "src": "backend/optimized_main_v2.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/optimized_main_v2.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/vercel/path0/backend"
  },
  "functions": {
    "backend/optimized_main_v2.py": {
      "runtime": "python3.9",
      "maxDuration": 30
    }
  }
}
